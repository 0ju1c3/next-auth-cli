{
  "name": "@0ju1c3-oss/next-auth-cli",
  "version": "1.0.3",
  "description": "CLI tool to automatically setup NextAuth.js authentication in Next.js App Router projects",
  "main": "./dist/index.js",
  "type": "module",
  "bin": {
    "next-auth-setup": "./bin/cli.js"
  },
  "scripts": {
    "build": "bun build src/cli.ts --outdir dist --target node --format esm",
