  "bugs": {
    "url": "https://github.com/0ju1c3/next-auth-cli/issues"
  },
  "homepage": "https://github.com/0ju1c3/next-auth-cli#readme",
  "dependencies": {
    "chalk": "^5.3.0",
    "fs-extra": "^11.2.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.10.5",
    "typescript": "^5.7.2"
  },
  "peerDependencies": {
    "next": ">=13.0.0",
    "next-auth": ">=5.0.0",
